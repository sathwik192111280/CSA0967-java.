import java.util.*;
class factors
{
public static void main(String[] agrs)
 {
 Scanner sc=new Scanner(System.in);
 System.out.println("ENTER A NUMBER");
 int n=sc.nextInt();
 int i,count=0;
 for(i=1;i<=n;i++)
 {
 if(n%i==0)
 count++; 
 }
 System.out.print("NUMBER OF FACTORS OF "+n+" IS "+count);
 }
}


output


ENTER A NUMBER
456
NUMBER OF FACTORS OF 456 IS 16
